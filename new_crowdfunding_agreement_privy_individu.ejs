  <style>
    /* General Styles */
    body {
      font-family: Calibri, Arial, sans-serif;
      line-height: 1.5;
      margin: 0;
      padding: 0;
    }

    /* A4 Page Layout with adjusted internal padding */
    .a4 {
      width: 210mm;
      margin: auto;
      background: white;
      padding-top: 25.4mm;
      padding-bottom: 25.4mm;
      padding-left: 25.4mm;
      padding-right: 40mm;
      overflow: hidden;
      display: flex;
      flex-direction: column;
      justify-content: flex-start;
      align-items: center;
    }

    /* Centered Content */
    .content {
      max-width: 180mm;
      text-align: justify;
    }

    h1 {
      text-transform: uppercase;
      font-family: Calibri, sans-serif;
      font-size: 11px;
      font-weight: 800;
      color: #333;
      margin: 10px 0;
      text-align: center;
    }

    h2 {
      text-align: center;
      color: #333;
      font-size: 11px;
    }

    ul {
      margin: 10px 0 10px 0;
      padding-left: 0;
      list-style-type: none;
      counter-reset: li;
    }

    ul li {
      position: relative;
      margin-left: 30px;
      font-family: Calibri, Arial, sans-serif;
      font-size: 11px;
      margin-bottom: 10px;
    }

    ul li::before {
      content: "("counter(li) ") ";
      counter-increment: li;
      position: absolute;
      left: -30px;
      font-weight: bold;
    }

    ol {
      margin: 10px 0 10px 0;
      padding-left: 0px;
      list-style-type: none;
      counter-reset: list-counter;
    }

    ol li {
      position: relative;
      margin-left: 30px;
      font-family: Calibri, Arial, sans-serif;
      font-size: 11px;
      margin-bottom: 10px;
    }

    ol li::before {
      content: counter(list-counter) ". ";
      counter-increment: list-counter;
      position: absolute;
      left: -30px;
      font-weight: bold;
    }

    ol.sub-list {
      margin: 10px 0 10px 0;
      padding-left: 0;
      counter-reset: sub-counter;
    }

    ol.sub-list li {
      position: relative;
      margin-left: 30px;
      font-family: Calibri, Arial, sans-serif;
      font-size: 11px;
      margin-bottom: 0px;
    }

    ol.sub-list li::before {
      content: counter(sub-counter, lower-alpha) ". ";
      counter-increment: sub-counter;
      position: absolute;
      left: -30px;
      font-weight: bold;
    }

    p {
      font-family: Calibri, Arial, sans-serif;
      font-size: 11px;
    }

    .signature {
      display: flex;
      justify-content: space-between;
      margin-top: 100px;
    }

    .signature div {
      width: 45%;
    }

    .signature .left {
      text-align: center;
    }

    .signature .right {
      text-align: center;
    }

    @page {
      size: A4;
      margin-top: 25.4mm;
      margin-bottom: 25.4mm;
    }

     /* Footer with page number */
    footer {
      position: absolute;
      bottom: 10mm;
      left: 50%;
      transform: translateX(-50%);
      font-size: 10px;
      color: #777;
